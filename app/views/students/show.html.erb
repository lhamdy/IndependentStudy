<h1> Student <%= @student.sid %></h1>

<h2> Previous courses: </h2>


<table>
    <thead>
        <tr>
            <th> Course ID </th>
            <th> Course Title </th>
            <th> Grade </th>
        </tr>
    </thead>
    <tbody>
    <% @courses.each do |course| %>
      <tr>
        <td><%= course.cid %></td>
        <td><%= course.title %></td>
        <td><%= course.grade %></td>
      </tr>
    <% end %>
  </tbody>

</table>

